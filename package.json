{
  "name": "workout-planner",
  "version": "1.0.0",
  "description": "A plugin for visualizing workout data with charts and graphs in Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node build-css.mjs && node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node build-css.mjs && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "workout",
    "fitness",
    "charts",
    "data-visualization"
  ],
  "author": "Rares Spatariu",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "esbuild-postcss": "^0.0.4",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-obsidianmd": "^0.1.5",
    "jest": "^30.2.0",
    "jest-environment-obsidian": "^0.0.1",
    "obsidian": "latest",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "ts-jest": "^29.4.5",
    "tslib": "2.4.0",
    "typescript": "~5.7.0",
    "typescript-eslint": "^8.46.1"
  }
}
